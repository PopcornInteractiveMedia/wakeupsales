  function quick_edit(id){    
    $.ajax({
      type: "POST",
      url: "/quick_deal",
      dataType: 'json',
      async: false,
      data: {deal_id: id},
      beforeSend: function(){
        $("#task_loader").show();
      },
      success: function(data)
      {
      },
      error: function(data) {
        $("#task_loader").hide();
      },
      complete: function(data) {
        $("#quickeditModal").modal("show");        
        $("#deal_content").html(data.responseText);
        $("#task_loader").hide();
        
      }
    });
  }
